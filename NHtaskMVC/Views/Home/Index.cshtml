@using NHtaskMVC.Models.DbEntities;
@model IEnumerable<User>

@{
    Layout = null;
    ViewData["Title"] = "User form";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width"/>
    <title>Index</title>
</head>
<body>
<h4>Customers</h4>
<hr/>
<table>
    <tr>
        <th>Phone</th>
        <th>Name</th>
        <th>Password</th>
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Phone</td>
            <td>@user.Name</td>
            <td>@user.Password</td>
        </tr>
    }
</table>
</body>
</html>
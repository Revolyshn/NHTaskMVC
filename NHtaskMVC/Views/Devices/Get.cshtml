@using NHtaskMVC.Models.DbEntities;
@model IEnumerable<Device>

@{
    Layout = null;
    ViewData["Title"] = "ABOBA";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width"/>
    <title>Index</title>
</head>
<body>
<h4>Devices</h4>
<hr/>
<table>
    <tr>
        <th>ID</th>
        <th>BLD</th>
        <th>UBG</th>
    </tr>
    @foreach (var device in Model)
    {
        <tr>
            <td>@device.ID</td>
            <td>@device.BLD</td>
            <td>@device.UBG</td>
        </tr>
    }
</table>
</body>
</html>